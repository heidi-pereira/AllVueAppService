{
  "name": "brandvue",
  "version": "1.0.0-beta",
  "description": "BrandVue brand-tracking, analysis, and visualisation.",
  "type": "module",
  "directories": {
    "lib": "lib"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "scripts": {
    "build:api": "nswag run ./GenerateTypescriptApi.nswag.json && nswag run ./GeneratePublicApiOpenApi3Spec.nswag.json",
    "build:dev": "webpack --config webpack.development.js",
    "analyze-dev-bundle": "webpack --config webpack.development.js --env BUNDLE_ANALYZE",
    "build": "webpack --config webpack.production.js",
    "analyze-bundle": "webpack --config webpack.production.js --env BUNDLE_ANALYZE",
    "build:docs": "pwsh ../BuildScripts/Build-Docs.ps1",
    "build-delayed:hotdev": "pwsh -Command \"Write-Host 'Waiting 20 seconds to give VS a chance to load'; Start-Sleep -Seconds 20; npm run build:hotdev\"",
    "build:hotdev": "node --max-http-header-size=100000 ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.development.js --hot",
    "build:hotdev_cert": "node --max-http-header-size=100000 ./node_modules/webpack-dev-server/bin/webpack-dev-server.js  --config webpack.development.js --hot --server-type https --server-options-pfx development.pfx --server-options-passphrase apples",
    "watch": "webpack --watch --config webpack.development.js",
    "test": "jest --maxWorkers=50%",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "deploy:debug": "pwsh ../BuildScripts/BuildDebugAndDeploy.ps1",
    "pre-build": "npm run install-cached && npm run build-cached:api",
    "install-cached": "pwsh ../BuildScripts/Run-IfChanged.ps1 BrandVue.FrontEnd/package.json BrandVue.FrontEnd/node_modules/.cache/Run-IfChanged npm install --prefer-offline",
    "build-cached:api": "pwsh ../BuildScripts/Run-IfChanged.ps1 BrandVue.FrontEnd/bin/BrandVue.dll BrandVue.FrontEnd/bin npm run build:api",
    "clean": "@('./node_modules/.cache/', './bin/') | Get-ChildItem -File | Where { $_.Name.EndsWith('.hash') } | Remove-Item",
    "web-build:dev": "npm run pre-build && npm run build:dev",
    "web-build:release": "npm run pre-build && npm run build && npm run build:docs",
    "pipelines-build": "npm run build:api && npm run build && npm run build:docs",
    "cy": "cypress open",
    "cy:run": "cypress run",
    "restore:config": "pwsh ../../local-scripts/Restore-LatestBrandVueMeta.ps1",
    "restore:config:beta": "pwsh ../../local-scripts/Restore-LatestBrandVueMeta.ps1 -containerDirectory 'testservers/sqltest$SQL2019/BrandVueMetaBeta/FULL/' -targetDatabaseName 'BrandVueMetaBeta'",
    "dbt:run": "dbt run --project-dir ../data_pipeline",
    "dbt:test": "dbt test --project-dir ../data_pipeline",
    "dbt:docs": "dbt docs generate --project-dir ../data_pipeline && dbt docs serve --project-dir ../data_pipeline"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MIG-Global/BrandVue.git"
  },
  "keywords": [
    "brand",
    "tracker"
  ],
  "author": "Alistair Cunningham, Bart Read, Graham The Coder, Mark Roberts",
  "license": "UNLICENSED",
  "private": true,
  "bugs": {
    "url": "https://app.shortcut.com/mig-global"
  },
  "homepage": "https://github.com/MIG-Global/BrandVue#readme",
